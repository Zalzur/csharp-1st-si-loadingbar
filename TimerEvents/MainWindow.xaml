<Window x:Class="TimerEvents.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TimerEvents"
        mc:Ignorable="d"
        Title="Loading..." Height="200" Width="500" Loaded="Window_Loaded">
    <Grid x:Name="Window" RenderTransformOrigin="0.5,0.5" Margin="0,0,12,27">
        <Grid.RenderTransform>
            <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform Angle="0.284"/>
                <TranslateTransform/>
            </TransformGroup>
        </Grid.RenderTransform>
        <ProgressBar x:Name="myProgressBar" Margin="40,69,30,47" ValueChanged="myProgressBar_ValueChanged"/>

    </Grid>
</Window>
